<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.kbw.maximalerfluss.Controller">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
      <children>
         <HBox fx:id="hbox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="1000.0" prefWidth="1000.0" style="-fx-border-color: blue;">
            <VBox.margin>
               <Insets bottom="20.0" />
            </VBox.margin>
         </HBox>
         <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
            <children>
               <Button fx:id="startknoten_setzen" mnemonicParsing="false" onAction="#startknotenSetzen" text="Startknoten setzen">
                  <HBox.margin>
                     <Insets right="10.0" />
                  </HBox.margin></Button>
               <Button fx:id="zielknoten_setzen" mnemonicParsing="false" onAction="#zielknotenSetzen" text="Zielknoten setzen">
                  <HBox.margin>
                     <Insets left="10.0" />
                  </HBox.margin>
               </Button>
            </children>
         </HBox>
      </children>
   </VBox>
   <HBox alignment="CENTER_RIGHT">
      <Label text="Anzahl Zeilen: " />
      <TextField fx:id="anzahl_zeilen" />
   </HBox>
   <HBox alignment="CENTER_RIGHT">
      <Label text="Anzahl Spalten: " />
      <TextField fx:id="anzahl_spalten" />
   </HBox>
   <HBox alignment="CENTER_RIGHT" layoutX="30.0" layoutY="595.0">
      <children>
         <Label fx:id="info" textFill="RED" />
      </children>
   </HBox>
   <HBox alignment="CENTER_RIGHT">
      <Button mnemonicParsing="false" onAction="#generate" text="generate" textAlignment="CENTER" />
   </HBox>
</VBox>
